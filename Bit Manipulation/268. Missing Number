class Solution {
    public int missingNumber(int[] nums) {
        int sum = 0;
        for(int num: nums){
            sum += num;
        }
        return ((nums.length) * (nums.length + 1))/2 - sum;
    }
}

OR

class Solution {
    public int missingNumber(int[] nums) {
        int res = nums.length;
        for(int i = 0; i < nums.length; i++){
            res = res ^ i ^ nums[i];
        }
        return res;
    }
}
