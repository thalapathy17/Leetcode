class Solution {
    public int[][] kClosest(int[][] points, int k) {
        PriorityQueue<int[]> minHeap = new PriorityQueue<>(Comparator.comparingInt(a -> a[0]*a[0] + a[1]*a[1]));
        for(int[] point : points){
            minHeap.add(point);
        }
        int[][] ans = new int[k][2];
        for(int i = 0; i < k; i++){
            ans[i] = minHeap.poll();
        }
        return ans;
    }
}
