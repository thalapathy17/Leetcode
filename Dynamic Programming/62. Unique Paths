class Solution {
    public int uniquePaths(int m, int n) {

        int[][] dp = new int[m + 1][n + 1];
        dp[m-1][n-1] = 1;

        for(int i = m-1; i >= 0; i--){
            for(int j = n-1; j >= 0; j--){
                dp[i][j] += dp[i+1][j] + dp[i][j+1];
            }
        }
        return dp[0][0];
    }
}

OR

class Solution {
    int[][] dp;
    public int uniquePaths(int m, int n) {
        
        dp = new int[m][n]; 
        return helper(m-1, n-1);
    }

    private int helper(int x, int y){
        if(x == 0 & y == 0) return 1;
        if(x < 0 || y < 0){
            return 0;
        }
        if(dp[x][y] != 0){
            return dp[x][y];
        }

        int res = helper(x - 1, y) + helper(x, y - 1);
        dp[x][y] = res;
        return res;
    }
}

