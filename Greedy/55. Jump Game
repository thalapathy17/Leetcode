class Solution {
    public boolean canJump(int[] nums) {
        int maxDist = 0;
        for(int i = 0; i < nums.length -1; i++){
            if(i > maxDist) return false;
            maxDist = Math.max(maxDist, i + nums[i]);
            
        }
        return maxDist >= nums.length -1 ? true: false;
    }
}
